.queue-container {
  background-color: #fff;
}

.divider {
  height: 20rpx;
  background-color: #f6f6f6;
}

.queue-list {
  display: flex;
  padding: 30rpx 0;
  flex-direction: row;
  font-size: 28rpx;
  color: #333;
}

.serial-tag {
  flex: 1;
  text-align: center;
}

.car-user-tag {
  flex: 2;
  text-align: center;
}

.queue-time-tag {
  flex: 2;
  text-align: center;
}

.queue-item {
  height: 80rpx;
  position: relative;
}

.item-detail {
  height: 100%;
  display: flex;
  flex-direction: row;
}

.serial-tag-item {
  width: 20%;
  text-align: center;
  line-height: 80rpx
}

.b-user-item {
  width: 40%;
  text-align: left;
  padding: 30rpx;
  box-sizing: border-box;
  line-height: 80rpx
}

.b-user-wrapper {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin: 0 30rpx
}

.queue-time-tag-item {
  width: 40%;
  text-align: center;
  line-height: 80rpx
}

.queue-user-avatar {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.serial-text {
  font-size: 28rpx;
  color: #666;
}

.user-name {
  flex: 1;
  font-size: 28rpx;
  color: #666;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap
}

.queue-start-time {
  font-size: 26rpx;
  color: #999;
}

.line {
  width: 95%;
  height: 1rpx;
  background-color: #ddd;
  position: absolute;
  bottom: 0;
  left: 5%;
}

.model-action {
  min-height: 150rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  box-shadow: 0 0 6px #ddd;
}

button {
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 8rpx;
  display: inline-block;
  font-size: 32rpx;
  padding: 0;
}

button::after {
  border: none;
}

.model-back {
  color: #fff;
  margin-left: 30rpx;
  background-color: #FF555C;
}

.model-sure {
  color: #fff;
  background-color: #AAAAAA;
}

.b-queue-container {
  margin: 40rpx 0;
}

.b-queue-cell {
  width: 50%;
  display: inline-block;
  text-align: center;
}

.b-queue-wait {
  text-align: center;
}

.b-queue-title {
  color: #333;
  font-size: 28rpx;
  margin-bottom: 10rpx;
}

.b-queue-wait-number {
  font-size: 48rpx;
}

.b-queue-normal-number {
  font-size: 28rpx;
  color: #333;
}
