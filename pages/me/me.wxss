page {
  background-color: #eee;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  background: linear-gradient(#fb653d, #fc3241);
  margin-bottom: 20rpx;
}

.cuntall {
  display: block;
  height: auto;
  margin-top: 10px;
  width: 100%;
  text-align: center;
  margin-bottom: 30px;
}

.total-con {
  display: block;
}

.total-con-num {
  display: block;
  margin-top: 10px;
}

.total-desc {
  font-size: 14px;
  color: #fff;
}

.total-num {
  font-size: 48px;
  color: #fff;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 30rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  margin: 20rpx;
  color: #300e0e;
}

.userinfo-dig {
  margin: 15rpx auto;
  padding: 20rpx 25rpx;
  border-radius: 10rpx;
  background-color: #f85f48;
  color: #fff;
  letter-spacing: 1px;
}

.userinfo-desc {
  color: #bbb;
  font-size: 14rpx;
  margin-bottom: 20rpx;
}

.profile-wrapper, .account-wrapper {
  background-color: #fff;
  margin-bottom: 20rpx;
}

.profile, .account {
  display: flex;
  padding: 30rpx;
  align-items: center;
  flex-direction: row;
  border-bottom: 1rpx solid #f0f0f0;
}

.profile-item, .account-item {
  flex: 1;
  font-size: 32rpx;
}

.profile-arrow, .account-arrow {
  width: 32rpx;
  height: 32rpx;
}

.share-button{
  display: flex;
  padding: 20rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  align-items: center;
  text-align: left;
  flex-direction: row;
  border-bottom: 1rpx solid #f0f0f0;
  background: none;
}

.share-button::after{
  border: none;
}

.btn-quit {
  width: 95%;
  margin: 0 auto;
  margin-top: 20px;
  border: none;
  background-color: #ff555c;
  color: #fff;
  margin-bottom: 20px;
}

.nav {
  margin-bottom: 20px;
  width: 100%;
  height: 70px;
  display: flex;
  flex-direction: row;
  color: #fff;
}

.boxView {
  display: flex;
  flex: 1;
  flex-direction: column;
  align-items: center;
}

.boxView image {
  margin-top: 7px;
  width: 36px;
  height: 36px;
}

.boxView text {
  font-size: 14px;
  margin-top: 7px;
}

.boxView .nav-num {
  font-size: 20px;
}

.itemicon{
  width:64rpx;
  height:64rpx;
  margin-right:20rpx;
  margin-left: 16rpx;
}

.deposit-text{
  color: #999;
  font-size: 28rpx;
}

.message_list {
  background-color: #f2f2f2;
  padding-left: 8rpx;
  padding-right: 8rpx;
}

.item2 {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 16rpx;
  background-color: #fff;
  /**  border: 1rpx solid #eaeaea;
  border-bottom: 1rpx solid #d0d0d0;
  border-radius: 5rpx;*/
}

.item-left {
  margin-top: 12rpx;
  display: table-cell;
  text-align: center;
}

.image {
  width: 128rpx;
  height: 128rpx;
  /* border-radius: 64rpx; */
  background-color: #eee;
}

.item-middle {
  flex: 1;
  margin-left: 30rpx;
  padding-top: 10rpx;
  overflow: hidden;
}

.title {
  color: #444;
  font-family: "微软雅黑";
  font-weight: bold;
  line-height: 20rpx;
  padding: 32rpx 16rpx, 0, 16rpx;
  font-size: 32rpx;
  margin-top: 60rpx;
  overflow: hidden; /*自动隐藏文字*/
  text-overflow: ellipsis; /*文字隐藏后添加省略号*/
}

.message {
  font-family: "微软雅黑";
  color: #888;
  line-height: 40rpx;
  font-size: 40rpx;
  padding: 0 16rpx, 0, 16rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-top: 16rpx;
  margin-bottom: 16rpx;
}

.item_right {
  overflow: hidden;
  padding-right: 30rpx;
  text-align: right;
  display:flex;
  align-items:center;
}
.item_right2{
  align-items: flex-end;
  margin-bottom: 10rpx;
}
.item_right2 text{
  font-size: 28rpx;
  color: #666;
}
.time {
  color: #03d0b7;
  font-size: 24rpx;
}

.mark {
  position: relative;
  left: 5rpx;
  margin-top: 10rpx;
  padding-top: 8rpx;
  padding-bottom: 2px;
  background-color: #f74c31;
  text-align: center;
  font-size: 18rpx;
  width: 40rpx;
  height: 30rpx;
  line-height: 30rpx;
  border-radius: 20rpx;
  color: #fff;
  display: table-cell;
}

.line {
  height: 1px;
  width: 100%;
  background-color: #e0e1e2;
}

.data-blt {
  padding-left: 3px;
  padding-right: 3px;
}

.data-text {
  color: #ff555c;
}

.myad-li {
  margin-top: 10rpx;
}

.btn-red {
  border: none;
  border-radius: 0 !important;
  background: #fb523e;
  color: #fff;
  font-size: 26rpx;
}

.btn-gray {
  border: none;
  border-radius: 0 !important;
  background: #ccc;
  color: #fff;
  font-size: 26rpx;
}

button::after {
  content: none;
}

