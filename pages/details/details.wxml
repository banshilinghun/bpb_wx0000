<view>
  <view class="page-section page-section-spacing swiper">
    <swiper indicator-dots="true" autoplay="true" interval="3000" duration="800" class="swiper-banner bg-white">
      <block wx:for="{{banners}}" wx:key="item.id">
        <swiper-item>
          <image src="{{item}}" mode="scaleToFill" style="width:100%;height:465.75rpx;"></image>
        </swiper-item>
      </block>
    </swiper>
  </view>
  <view class="profile-wrapper">
    <block>
      <view class="profile frpro">
        <view class="profile-item cwsfts">{{adInfo.name}}</view>
      </view>
    </block>
    <block>
      <view class="profile">
        <view class="profile-item">广告类型:</view>
        <view class="right-text">
          <label class="data-text" wx:if="{{adInfo.type==0}}">车尾</label>
          <label class="data-text" wx:if="{{adInfo.type==1}}">车内</label>
          <label class="data-text" wx:if="{{adInfo.type==2}}">车尾+车内</label>
          <label class="data-text" wx:if="{{adInfo.type==3}}">车内+车外</label>
          <label class="data-text" wx:if="{{adInfo.type==4}}">车外</label>
        </view>
      </view>
    </block>
    <block>
      <view class="profile">
        <view class="profile-item">实际收益:</view>
        <view class="right-text" wx:if='{{adInfo.amount!=null}}'>{{adInfo.amount}}元</view>
        <view class="right-text" wx:if='{{adInfo.amount==null}}'>身份认证后可查看收益</view>
      </view>
    </block>
    <block>
      <view class="profile">
        <view class="profile-item">投放车辆:</view>
        <view class="right-text">{{adInfo.total_count}}辆</view>
      </view>
    </block>
    <block>
      <view class="profile">
        <view class="profile-item">投放时间:</view>
        <view class="right-text">{{adInfo.begin_date}}
          <label class="data-blt">~</label>{{adInfo.end_date}}</view>
      </view>
    </block>
  </view>
  <block wx:if="{{checkPlan}}">
    <view class="profile-wrapper" bindtap="checkPlan" data-adId="{{adInfo.id}}">
      <view class="profile frpro">
        <view class="profile-item cwsfts">检测计划</view>
        <image class="profile-arrow" src="/image/arrow.png"></image>
      </view>
    </view>
  </block>
    <view class="profile-wrapper protwo">
    <block wx:if="{{service.length>0}}">
      <view class="profile frpro">
        <view class="profile-item cwsfts">预约前往下列服务站</view>
      </view>
    </block>
    <block wx:for="{{service}}" wx:for-index="index" wx:key="item.id">
      <view class="item">
        <view class="item-middle">
          <view>
            <text class="title">{{item.name}}</text>
          </view>
          <view class="myad-li">
            <view class="message">
              <image src='../../image/dw.png'></image>{{item.address}}</view>
            <view class="mmtext">剩余车辆
              <label class="province-text">{{item.ad_count-item.subscribe_count}}辆</label>
               <label class="province-text txtstx" wx:if='{{item.lista==2}}'>已预约{{selDate}} {{selTime}}</label> 
            </view>
          </view>
        </view>
        <view class="item_right">
          <view>
            <text class="time">{{item.distance}}km</text>
          </view>
          <view class="orgtext" bindtap="goMap" data-longitude="{{item.lng}}" data-latitude="{{item.lat}}" data-name="{{item.name}}" data-address="{{item.address}}">
            <image src='../../image/dh.png'></image>导航
          </view>
          <view class="fastwx" wx:if='{{item.lista==1}}' bindtap="arrangement" data-adId="{{adId}}" data-serverId="{{item.id}}"> 预约</view>
          <view class="fastwx" wx:if='{{item.lista==2}}' bindtap="cancel"> 取消</view>
          <view class="fastwx2" wx:if='{{item.lista==0}}' > 预约</view>
        </view>
      </view>
    </block>
  </view>
