.test {
  width: 100rpx;
  height: 100rpx;
  position: fixed;
  background-color: green;
  border-radius: 50%;
  top: 0;
  left: 0;
}

.table-wrapper{
  width: 100%;
  height: 300rpx;
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  border-left: 1rpx solid #ddd;
  border-top: 1rpx solid #ddd;
}

.table-title-color{
  background-color: #ff555c;
}

.table-content-color{
  background-color: #fff;
}

.rule-table{
  width: 50%;
  height: 20%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  box-sizing: border-box;
  align-items: center;
  border-bottom: 1rpx solid #ddd;
  border-right: 1rpx solid #ddd;
}

.rule-table-text{
  color: #333;
  font-size: 20rpx;
}