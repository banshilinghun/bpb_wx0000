/**app.wxss**/
@import 'components/index.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
/*统一的按钮样式  */

.btn-common {
  background: none;
  line-height: 60rpx;
  height: 60rpx;
  font-size: 32rpx;
  color: #ff555c;
  padding: 0;
  background-color: #fff;
  border-radius: 6rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  position: absolute;
  right: 0rpx;
  border: 2rpx solid #ff555c;
  font-weight: 500;
}
